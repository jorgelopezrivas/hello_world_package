{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
      "jest": true,
      "es6":true,
      "browser": true,
      "amd": true,
      "node": true
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "no-use-before-define": "off",
      "react/jsx-filename-extension": "off",
      "react/prop-types": "off",
      "comma-dangle": "off"
    },
    "globals": {
      "fetch": false
    }
}